{ 
    "version": 2, 
    "builds": [
        { "src": "index.js", "use": "@vercel/node" },
        { "src": "public/**", "use": "@vercel/static" }
    ], 
    "rewrites": [
        { "src": "/uploads/(.*)", "dest": "/public/uploads/$1" },
        { "src": "/web/(.*)", "dest": "/public/$1" },
        { "src": "/(.*\\.(jpg|jpeg|png|gif|svg|html|css|js))", "dest": "/public/$1" },
        { "src": "/(.*)", "dest": "/index.js" }
    ] 
}